<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="edureka">
   <parameter name="browser" value="chrome" />
   <parameter name="URL" value="https://www.edureka.co/" />
   <test thread-count="3" name="Home Page">
      <classes>
         <class name="com.edureka.test.Home">
            <methods>
               <include name="login" />
               <include name="loginOverloadedMethod" />
            </methods>
         </class>
         <class name="com.edureka.test.TrendingCourses">
            <methods>
               <include name="validateTile" />
            </methods>
         </class>
         <class name="com.edureka.test.CareerRelated">
            <methods>
               <include name="validateTile" />
            </methods>
         </class>
         <class name="com.edureka.test.Course">
            <methods>
               <include name="coursePage" />
            </methods>
         </class>
      </classes>
   </test>
   <!-- Test -->
</suite>